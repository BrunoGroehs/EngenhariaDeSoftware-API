{
  "all": true,
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "src/server.js",
    "test/**/*",
    "coverage/**/*",
    "node_modules/**/*"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov"
  ],
  "report-dir": "coverage",
  "temp-dir": ".nyc_output",
  "cache": true,
  "check-coverage": true,
  "lines": 60,
  "statements": 60,
  "functions": 60,
  "branches": 60
}
